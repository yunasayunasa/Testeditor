{
  "initialState": "Novel",
  "states": {
    "Novel": {
      "onEnter": [
        {
          "type": "run_scene",
          "params": {
            "sceneKey": "GameScene",
            "params": {
               "startScenario": "test"
            }
          }
        }
      ],
      "onExit": [
        {
          "type": "stop_scene",
          "params": {
            "sceneKey": "GameScene"
          }
        }
      ],
      "transitions": [
        {
          "event": "start_gameplay",
          "target": "Gameplay"
        }
      ]
    },
    "Gameplay": {
      "onEnter": [
        {
          "type": "run_scene",
          "params": {
            "sceneKey": "JumpScene"
          }
        }
      ],
      "onExit": [
        {
          "type": "stop_scene",
          "params": {
            "sceneKey": "JumpScene"
          }
        }
      ],
      "transitions": [
        {
          "event": "return_to_novel",
          "target": "Novel"
        }
      ]
    }
  }
}