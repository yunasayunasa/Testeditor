{
  "type": "Container",
  "name": "template_root",
  "width": 500,
  "height": 60,
  "objects": [
    {
      "name": "background",
      "type": "Image",
      "texture": "button_texture",
      "originX": 0.5, "originY": 0.5,
      "x": 250, "y": 30
    },
    {
      "name": "item_name",
      "type": "Text",
      "x": 250,
      "y": 30,
      "text": "Item Name",
      "style": { "fontSize": "24px", "fill": "#000000" },
      "originX": 0.5,
      "originY": 0.5
    }
  ],
 "events": [
  {
    "trigger": "onClick",
    "id": "event_select_item",
    "nodes": [
      {
        "id": "node_set_var",
        "type": "set_variable",
        "params": {
          "var": "f.selected_evidence",
          "value": "&{data.id}" 
        }
      },
      {
        "id": "node_close",
        "type": "close_menu",
        "params": {}
      }
    ],
    "connections": [
      { "fromNode": "start", "fromPin": "output", "toNode": "node_set_var", "toPin": "input" },
      { "fromNode": "node_set_var", "fromPin": "output", "toNode": "node_close", "toPin": "input" }
    ]
  }
]}